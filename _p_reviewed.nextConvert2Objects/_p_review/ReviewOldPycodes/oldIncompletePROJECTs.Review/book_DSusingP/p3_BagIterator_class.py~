#
class _BagIterator:
	def __init__( self, theList ):
		self._bagItems = theList
		self._curItem = 0
	
	def __iter__( self ):
		return self
		
	def __next__( self ):
		if self._curItem < len( self._bagItems ):
			item = self._bagItems[ self._curItem ]
			self._curItem += 1
			return item
		else:
			raise StopIteration
